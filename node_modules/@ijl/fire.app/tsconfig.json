{
    "compilerOptions": {
        "lib": [
            "dom",
            "es7"
        ],
        "outDir": "./dist/",
        "sourceMap": true,
        "esModuleInterop": true,
        "downlevelIteration" :true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "typeRoots": ["node_modules/@types", "src/typings", "../../node_modules/@types"]
    },
    "exclude": [
        "node_modules"
    ]       
}